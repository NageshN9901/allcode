
public class Bike {

	String name;
	String color;
	int price;
	int tax;
	
	Bike(String n,String c,int p,int t)
	{
		this.name=n;
		this.color=c;
		this.price=p;
		this.tax=t;
	
	}
	void display()
	{
	System.out.println("name;"+this.name);	
	System.out.println("color;"+this.color);	
	System.out.println("sellingPrice;"+this.price+this.tax);	
		
	}
	

	public static void main(String[] args) {
		Bike b1=new Bike("tajmahal","white",1250);
		Bike b2=new Bike("whitehouse","yellow",1250);
                         b1.display();
	}

}
