
public class FanIMPL0 implements Iswtich0Step1,Iregulator0Step2, Inff20Step3 ,Inf0 {

	//here inf also inculded
	@Override
	public void increasespeed() {
		System.out.println("fan speed increase");
	}
	
	@Override
	public void reducespeed() {
		
		System.out.println("fan speed reduce");
	}
	
	@Override
	public void switchon() {
		System.out.println("fan swtich on");
		
	}
	@Override
	public void switchoff() {
		System.out.println("fan swtich on");
		
	}

	
	public void repir() {

System.out.println("hi all");
		
	}

	//4 by class implementing all interface by ,
}
