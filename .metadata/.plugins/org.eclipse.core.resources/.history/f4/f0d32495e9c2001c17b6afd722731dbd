
public class Sample {

	static
	{
		System.out.println("hello");
	}
	
	static
	{
		
		System.out.println("jhdeuuh");
	
	}
	
	
	String name;
	int price;
	Sample(String n,int p )
	{
		
		
		this.name=n;
		this.price=p;
	}
	
	public static void main(String[] args) {
	
	}
	
}
// without object we can call static mathpod by invoking as static