
public class WrapperclassDemo {
// jdk 1.8
	
	
	
	
	public static void main(String[] args) {
		
		
		byte b=10;
		short s=100;
		int i=1000;
		long l=10000000000000000l;
		float f=10.0f;
		double d=100.0;
	 boolean x=true;
	 char c='c';
	 //auto conversion from primtive data to non primative data 
	 Object z[]= {b,s,i,l,f,d,x};
	 for(int p=0;i<z.length;i++)
	 {
		System.out.println(z[i]);
	 }
	 
		
		
	}
}
