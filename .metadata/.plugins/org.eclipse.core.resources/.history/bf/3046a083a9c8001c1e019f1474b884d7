
public interface Inf extends Iswtich,Iregulator,Inff2{


	@Override
	public void increasespeed(); 
	
	@Override
	public void reducespeed(); 
	
	@Override
	public void switchon(); 
	@Override
	public void switchoff();

	public  static void repir(){
		System.out.println("hi all");
	}


	
	////this is sub interface can inherte multiplainterface using keywaord extendds
	
	
	}
