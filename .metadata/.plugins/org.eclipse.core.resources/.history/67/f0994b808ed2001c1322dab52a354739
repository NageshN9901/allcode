


	import java.util.Scanner;
	public class SumDivsiors {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		//System.out.println("enter  number value");
		int x=sc.nextInt();
		
		
	}
	//int sum=0;
		int Sumod(int n)
		{
			
			
		
			int sum=n;
			for (int i=1; i<=n/2; i++)
			{
				
				if(n%i==0)
			
					sum=sum+i;
			}

		
	return sum;
			
	
			}
	
}
