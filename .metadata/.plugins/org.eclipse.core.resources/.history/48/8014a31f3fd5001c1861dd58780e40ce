import java.util.Scanner;
static int getprime(int n)
{
	
	
	for(int i=1; ;i++)
	{
		
		
		boolean rs=isprime(i);
				if(rs=true)
				{
					n--;
					if(n==0)
						return i;
				}
	}
}
public class Getprime {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("enter number");
		int n=sc.nextInt();
		getprime(n);
	}
}