
public class NonstaticStudent {



	String name;
	 String course="dev";
	int Eid =12323;
	
	
	public static void main(String[] args) {
		Student s1=new Student();
		s1.name="nagesh";
		Student s2=new Student();
		s2.name="mahesh";
		Student s3=new Student();
		s3.name="ramesh";
		s2.course="python";  // chnage it will change for all objects
		
		System.out.println(s1.name+"  "+s1.course+" "+s1.Eid);
		System.out.println(s2.name+"  "+s2.course+" "+s2.Eid);
		System.out.println(s3.name+"  "+s1.course+" "+s3.Eid);
	}
	
}

}
