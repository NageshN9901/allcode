import java.util.Scanner;

public class ChcekARm {
public static void main(String[] args) {
	

	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
	String st=check(n);
	System.out.println(n +"mbet is");
	
static String check(int input)
{
int temp=input,sum=0;
int dc=input+" ".length();
do {
	
	
	int r=input%10;
	sum=sum+(int)Math.pow(r, dc);
	inout=(input/10);
}while(input!=0);
if(sum==temp)
	return  "yes";
else
	return   "no";
}
}


}



