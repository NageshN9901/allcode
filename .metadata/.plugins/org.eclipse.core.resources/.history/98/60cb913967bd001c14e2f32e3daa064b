
public class NonstaticStudent {



	String name;
	 String course;
//	int Eid =12323;
	 
	
	public static void main(String[] args) {
		Student s1=new Student();
		s1.name="nagesh";  s1.course="ggg";
		Student s2=new Student();
		s2.name="mahesh"; s2.course="eee";
		Student s3=new Student();
		s3.name="ramesh"; s3.course="engdev";
		  // chnage it will change for all objects
		
		System.out.println(s1.name+"  "+s1.course);
		System.out.println(s2.name+"  "+s2.course);
		System.out.println(s3.name+"  "+s1.course);
	}
	
}


