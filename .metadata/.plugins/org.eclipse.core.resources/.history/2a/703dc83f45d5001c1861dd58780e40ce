import java .util.Scanner;
public class Mianfeb {
	

	static void printfib(int n)
	{
		
		int f1=0,f2=1;
		  
		while(f1<=n) {
		
			
		
			int f3=f1+f2;
			f1=f2;
			f2=f3;
		}
	}
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		// need one mthod to perform ome tyask or logic
		printfib(n);
	}

}
