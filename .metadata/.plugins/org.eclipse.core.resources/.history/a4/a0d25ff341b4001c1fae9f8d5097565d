
public class Methodadvacncechain {
	
	String name;
	String color;
	int price;
	int tax;
	
	
	Bike(String n,String c,int p,int t)
	{
		this.name=n;
		this.color=c;
		this.price=p;
		this.tax=t;
	
	
	
	void sellingprice()
	{
		int sp=this.price+this.tax;
		System.out.println("sellingprice; "+sp);
	}
	void display()
	{
		System.out.println("name; "+this.name);
		System.out.println("color; "+color);
		System.out.println(sellingprice);
		
	}

	public static void main(String[] args) {
	

	}

}
