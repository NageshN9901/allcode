
public class Car {
String name,color;



Car(String n,String t)
{
	this.name=n;
	
	this.color=t;
}

void drive()
{
	
	System.out.println("Drive car");
}



}
class bike{
	String name,color;
	bike(String n,String t)
	{
		this.name=n;
		
		this.color=t;
	}

	void ride()
	{
		
		System.out.println("Drive bike");
	}
	
}



