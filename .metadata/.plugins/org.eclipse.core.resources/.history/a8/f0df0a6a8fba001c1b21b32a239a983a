
public class MethodreturNonPubg extends MethodreturnNonPub {
	
	
	Gun button1()
	{
	Gun g=new Gun();
		return g;
	}
	
	Pan button2() { 
		
		Pan p=new Pan();
		return p;
		}
	Bomb button3()
	{
		Bomb b=new Bomb();
		return b;	
	}
	
	
	
	public static void main(String[] args) {
		
		MethodreturNonPubg m=new MethodreturNonPubg();
		m.button1();
		System.out.println(m.button1());
		m.button2();
		m.button3();
		
	}
}
