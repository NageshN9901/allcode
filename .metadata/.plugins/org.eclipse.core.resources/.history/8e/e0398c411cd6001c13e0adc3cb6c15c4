
	import java.util.Scanner;

public class ninth {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("enter number lines number");
		int n=sc.nextInt();
		
		// outer loop for row
		for (int i=n; i>=1; i--) {
			
			// inner llop for column
			for (int j=n; j>=1;j--) {
				
						
					System.out.print(i);
				}// for space between 1 and n
			System.out.println();	
				
			}
				
		}
	}
	





}
