
public class DataLoss {
public static void main(String[] args) {
	int i=129;
	byte b=(byte)i;
	System.out.println(i+" "+b);     //129 -127    
	
	
	double d=10.22;
	int x=(int)d;
	System.out.println(d+" "+x);  //10.22 10
	
	//here loss of .22 decmal value due to converting from decaml double to non int
	
	
}
}
