
public class CpuIMPL implements iMOUSE{
  @Override
	public void Click() {
		
		System.out.println("appe/are get selected");
	}
  @Override
public void doubleClick() {
		
		System.out.println("app/redsousree gets opedm");
	}
  @Override
public void rightClick() {
	
	System.out.println("display option");
}

	
	public static void main(String[] args) {
		iMOUSE M=new  CpuIMPL ();
		M.Click();
		M.doubleClick();
		M.rightClick();
	}
}
