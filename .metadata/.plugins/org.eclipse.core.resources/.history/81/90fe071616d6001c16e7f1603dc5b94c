





	import java.util.Scanner;
	public class seventh { 
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("enter number lines number");
		int n=sc.nextInt();
		
		// outer loop for row
		for (int i=1; i<=n; i++) {
			int num=i%2==0 ?0:1;
			System.out.print(num);//101010
	}
	}
}
